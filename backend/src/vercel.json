{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/routes/calcAndSearch.routes.js"
    },
    {
      "src": "/api/v1/calcAndSearch/(.*)",
      "dest": "src/routes/calcAndSearch.routes.js"
    }
  ]
}